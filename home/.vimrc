
map # <Nop>
" let mapleader = '#'

" Plugins
" source ~/.config/vim/plugins.vim

" Sets
" source ~/.config/vim/sets.vim
" set undodir=~/tmp/vim/undo-vim
" set undofile
" set viminfo='100,n$HOME/.vim/files/info/viminfo

"  Plugin Configs
" source ~/.config/vim/configs.vim

"  Color Settings
" source ~/.config/vim/color.vim

" FZF
" source ~/.config/vim/fzf.vim

" NERDTree
" source ~/.config/vim/nerdtree.vim

" GOLANG
" source ~/.config/vim/go.vim

" Mappings
" source ~/.config/vim/mappings.vim

" Airline
" source ~/.config/vim/airline.vim

" Abbreviations
" source ~/.config/vim/ab.vim

" Snippets
" source ~/.config/vim/snippets.vim

" Commands
" source ~/.config/vim/commands.vim

" TODO
" source ~/.config/vim/TODO.vim

" set foldmethod=marker
" let &t_SI = "\e[6 q"
" let &t_EI = "\e[2 q"

" reset the cursor on start (for older versions of vim, usually not required)
" augroup myCmds
" au!
" autocmd VimEnter * silent !echo -ne "\e[2 q"
" augroup END
" set bg=dark

