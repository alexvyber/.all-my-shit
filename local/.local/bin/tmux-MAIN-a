#!/usr/bin/env bash

session=ONE
window=MAIN
# If we don't have main session, we create it with $window at index 1
 ! [  tmux has-session -t "=$session" 2> /dev/null ] && tmux new-session -d -s $session -n $window -c "~"

# Go to the destenition
[ ! $TMUX ] && tmux a -t $session:$window

# If we don't have $window window, we create it
if ! tmux list-windows -t $session | grep $window 2> /dev/null ; then
  tmux switch -t $session
  tmux new-window -n $window -c "~"
  exit 0
else
  tmux switch -t $session:$window
  exit 0
fi
